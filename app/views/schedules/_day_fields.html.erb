<fieldset>
	
	<div class="form-group">
		<%= f.label :start_time, class: "control-label col-md-2 col-md-offset-3" %> 
		<div class="col-md-2"><%= f.datetime_select :start_time, nil, {placeholder: "Click to add time"} %></div>
	</div>

	<div class="form-group">
		<%= f.label :end_time, class: "control-label col-md-2 col-md-offset-3" %> 
		<div class="col-md-2"><%= f.datetime_select :end_time, nil, {placeholder: "Click to add time"} %></div>
		<%= f.hidden_field :_destroy %>
		<div class="col-md-1"><%= link_to "Remove", "#", class: "remove_fields btn btn-default" %></div>
	</div>

</fieldset>