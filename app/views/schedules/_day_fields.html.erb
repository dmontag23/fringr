<fieldset>
	<%= f.label :start_time %> 
	<%= f.datetime_select :start_time, :ampm => true, :minute_step => 5 %>
	<%= f.label :end_time %> 
	<%= f.datetime_select :end_time, :ampm => true, :minute_step => 5 %> 
	<%= f.hidden_field :_destroy %>
	<%= link_to "Remove", "#", class: "remove_fields" %>
</fieldset>