<tr id="schedule-<%= schedule.id %>" >
  <td><%= link_to schedule.name, schedule, method: :get, class: "btn btn-default" %></td>
  <td class="right"><%= link_to "Edit", edit_schedule_path(schedule), method: :get, class: "btn btn-primary" %>
  <%= link_to "Delete", schedule, method: :delete, class: "btn btn-danger",
                                  data: { confirm: "You sure?" } %></td>
</tr>