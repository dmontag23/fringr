<div class="container">
	<h1> Welcome to Fringr <%= current_user.name %>!</h1>
	<div class="row">
		<div class="col-md-4 center">
			<%= link_to "New Schedule", new_schedule_path, class: "btn btn-default" %>
		</div>
		<div class="col-md-4 center">
			<%= link_to "Contacts", contacts_path, class: "btn btn-default" %>
		</div>
		<div class="col-md-4 center">
			<%= link_to "Locations", locations_path, class: "btn btn-default" %>
		</div>
	</div>
	<div class="center"><h3>Schedules</h3></div>
	<div class="row">
		<div class="col-md-6 col-md-offset-3">
			<table class="schedules table table-striped table-hover">
				<th colspan=2>Name</th>
				<%= render @schedules %>
			</table>
		</div>
	</div>
	<%= will_paginate @schedules %>
</div>
